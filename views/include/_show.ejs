<% for(var i=0; i < users.length; i++) { %>
    <tr>
        <td>
            <%= i + 1 %>
        </td>
        <td>
            <%= users[i].number %>
        </td>
        <td>
            <%= users[i].model %>
        </td>
        <td>
            <%= users[i].type %>
        </td>
        <td>
            <%= users[i].phone %>
        </td>
        <td>
            <%= users[i].check %>
        </td>

        <td>
            <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient">Check out</span>
            </a>
            <a class="btn border-shadow delete" data-id=<%=users[i]._id%> >
                <span class="text-gradient">Pay</span>
            </a>
        </td>
    </tr>
    <% } %>